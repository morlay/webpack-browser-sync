{
  "name": "webpack-browser-sync",
  "version": "0.7.0",
  "main": "lib/index.js",
  "description": "A webpack dev server with browser-sync",
  "bin": {
    "webpack-browser-sync": "bin/webpack-browser-sync.js"
  },
  "files": [
    "src/",
    "lib/",
    "bin/"
  ],
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "build": "tsc -p .",
    "webpack": "ts-node ./src/command.ts --config ./src/__examples__/webpack.config.ts --index=../index.html",
    "lint": "tslint ./ --fix",
    "test": "npm run lint && npm run build && ava -v",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@types/browser-sync": "0.0.x",
    "@types/compression": "0.0.x",
    "@types/lodash": "4.14.x",
    "@types/node": "7.0.x",
    "@types/webpack": "2.2.x",
    "@types/webpack-dev-middleware": "1.9.x",
    "@types/yargs": "6.6.x",
    "browser-sync": "2.18.x",
    "compression": "1.6.x",
    "connect-history-api-fallback": "1.3.x",
    "interpret": "1.0.x",
    "lodash": "4.17.x",
    "memory-fs": "0.4.x",
    "webpack-dev-middleware": "1.10.x",
    "webpack-hot-middleware": "2.18.x",
    "yargs": "8.0.x"
  },
  "peerDependencies": {
    "webpack": "2"
  },
  "devDependencies": {
    "@types/faker": "4.1.0",
    "@types/html-webpack-plugin": "2.28.0",
    "ava": "0.19.x",
    "faker": "4.1.x",
    "html-webpack-plugin": "2.28.x",
    "ts-loader": "2.1.x",
    "ts-node": "3.0.x",
    "tslint": "5.4.x",
    "typescript": "2.3.x",
    "webpack": "*"
  },
  "repository": "git@github.com:morlay/webpack-browser-sync.git",
  "author": "Morlay <morlay@qq.com>",
  "license": "WTFPL"
}
